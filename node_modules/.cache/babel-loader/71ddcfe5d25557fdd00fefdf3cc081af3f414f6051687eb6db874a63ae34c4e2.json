dPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard-page\">\r\n    <div class=\"dashboard-header\">\r\n      <h1>Dashboard</h1>\r\n      <p>Welcome, {{ userDisplayName }}!</p>\r\n    </div>\r\n\r\n    <div v-if=\"loading\" class=\"loading-state\">\r\n      <div class=\"spinner-large\"></div>\r\n      <p>Loading dashboard...</p>\r\n    </div>\r\n\r\n    <div v-else-if=\"errorMessage\" class=\"error-state\">\r\n      <div class=\"alert alert-error\">\r\n        <strong>Error:</strong> {{ errorMessage }}\r\n      </div>\r\n      <div class=\"error-actions\" style=\"display:flex;gap:8px;justify-content:center;margin-top:12px;\">\r\n        <button @click=\"fetchDashboard\" class=\"btn btn-secondary\">Retry</button>\r\n        <button @click=\"useMockData\" class=\"btn btn-secondary\">Use sample data</button>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else class=\"dashboard-content\">\r\n      <!-- Quick Stats -->\r\n      <div class=\"stats-grid\">\r\n        <div class=\"stat-card\">\r\n          <div class=\"stat-icon alert-icon\">üè†</div>\r\n          <div class=\"stat-info\">\r\n            <p class=\"stat-label\">Total Evacuation Areas</p>\r\n            <p class=\"stat-value\">{{ dashboardData.total_evacuation_areas || 0 }}</p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"stat-card\">\r\n          <div class=\"stat-icon family-icon\">üë®‚Äçüë©‚Äçüëß‚Äçüë¶</div>\r\n          <div class=\"stat-info\">\r\n            <p class=\"stat-label\">Families Evacuated</p>\r\n            <p class=\"stat-value\">{{ dashboardData.families_evacuated || 0 }}</p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"stat-card\">\r\n          <div class=\"stat-icon location-icon\">üë•</div>\r\n          <div class=\"stat-info\">\r\n            <p class=\"stat-label\">Total People</p>\r\n            <p class=\"stat-value\">{{ dashboardData.total_people || 0 }}</p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"stat-card\">\r\n          <div class=\"stat-icon facility-icon\">‚úÖ</div>\r\n          <div class=\"stat-info\">\r\n            <p class=\"stat-label\">Available Areas</p>\r\n            <p class=\"stat-value\">{{ dashboardData.available_areas || 0 }}</p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"stat-card\">\r\n          <div class=\"stat-icon alert-icon\">‚ùå</div>\r\n          <div class=\"stat-info\">\r\n            <p class=\"stat-label\">Full Areas</p>\r\n            <p class=\"stat-value\">{{ dashboardData.full_areas || 0 }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Recent Alerts Section -->\r\n      <div class=\"section\">\r\n        <h2>Recent Disaster Alerts</h2>\r\n        <div v-if=\"dashboardData.recent_alerts && dashboardData.recent_alerts.length > 0\" class=\"alerts-list\">\r\n          <div v-for=\"alert in dashboardData.recent_alerts\" :key=\"alert.id\" class=\"alert-item\">\r\n            <div class=\"alert-severity\" :class=\"`severity-${alert.severity}`\"></div>\r\n            <div class=\"alert-content\">\r\n              <h4>{{ alert.title }}</h4>\r\n              <p>{{ alert.description }}</p>\r\n              <span class=\"alert-time\">{{ formatDate(alert.created_at) }}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div v-else class=\"empty-state\">\r\n          <p>No recent alerts</p>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Family Status Section -->\r\n      <div class=\"section\">\r\n        <h2>Family Status</h2>\r\n        <div v-if=\"dashboardData.family && dashboardData.family.length > 0\" class=\"family-grid\">\r\n          <div v-for=\"member in dashboardData.family\" :key=\"member.id\" class=\"family-card\">\r\n            <div class=\"family-avatar\">{{ member.name.charAt(0).toUpperCase() }}</div>\r\n            <div class=\"family-info\">\r\n              <p class=\"family-name\">{{ member.name }}</p>\r\n              <p class=\"family-status\">{{ member.relationship }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div v-else class=\"empty-state\">\r\n          <p>No family members registered</p>\r\n          <router-link to=\"/resident_pages/family\" class=\"btn btn-secondary btn-sm\">Add Family Member</router-link>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Quick Actions -->\r\n      <div class=\"section\">\r\n        <h2>Quick Actions</h2>\r\n        <div class=\"actions-grid\">\r\n          <router-link to=\"/resident_pages/evacuation\" class=\"action-card\">\r\n            <div class=\"action-icon\">üö™</div>\r\n            <p>Start Evacuation</p>\r\n          </router-link>\r\n          <router-link to=\"/resident_pages/family\" class=\"action-card\">\r\n            <div class=\"action-icon\">üë•</div>\r\n            <p>Manage Family</p>\r\n          </router-link>\r\n          <router-link to=\"/resident_pages/facilities\" class=\"action-card\">\r\n            <div class=\"action-icon\">üè†</div>\r\n            <p>Find Shelter</p>\r\n          </router-link>\r\n          <router-link to=\"/resident_pages/predictions\" class=\"action-card\">\r\n            <div class=\"action-icon\">üìä</div>\r\n            <p>View Predictions</p>\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { reactive, ref, onMounted } from 'vue';\r\nimport { residentDashboardService, residentEmergencyAlertsService, residentUpdatesService, residentPredictionsService } from '../../services/residentService';\r\n\r\nconst loading = ref(false);\r\nconst errorMessage = ref('');\r\nconst userDisplayName = ref('User');\r\n\r\nconst dashboardData = reactive({\r\n  total_evacuation_areas: 0,\r\n  families_evacuated: 0,\r\n  total_people: 0,\r\n  available_areas: 0,\r\n  full_areas: 0,\r\n  emergency_alerts: [],\r\n  recent_disaster_updates: [],\r\n  active_predictions: []\r\n});\r\n\r\nonMounted(() => {\r\n  const user = JSON.parse(localStorage.getItem('resident_user') || '{}');\r\n  userDisplayName.value = user.name || 'Resident';\r\n  fetchDashboard();\r\n});\r\n\r\nconst fetchDashboard = async () => {\r\n  loading.value = true;\r\n  errorMessage.value = '';\r\n\r\n  try {\r\n    // Fetch dashboard stats\r\n    const dashboardResponse = await residentDashboardService.getDashboard();\r\n    if (dashboardResponse && dashboardResponse.status === 200 && dashboardResponse.data && dashboardResponse.data.stats) {\r\n      const stats = dashboardResponse.data.stats;\r\n      dashboardData.total_evacuation_areas = stats.total_evacuation_areas || 0;\r\n      dashboardData.families_evacuated = stats.total_families || 0;\r\n      dashboardData.total_people = stats.total_members || 0;\r\n      dashboardData.available_areas = stats.available_evacuation_areas || 0;\r\n      dashboardData.full_areas = (stats.total_evacuation_areas || 0) - (stats.available_evacuation_areas || 0);\r\n    } else {\r\n      errorMessage.value = 'Dashboard endpoint returned no data. Check System-E API at http://localhost:8001/api/dashboard.';\r\n      console.warn('Dashboard returned empty:', dashboardResponse);\r\n    }\r\n\r\n    // Fetch emergency alerts\r\n    const alertsResponse = await residentEmergencyAlertsService.getEmergencyAlerts();\r\n    if (alertsResponse && alertsResponse.status === 200 && alertsResponse.data) {\r\n      dashboardData.emergency_alerts = Array.isArray(alertsResponse.data) ? alertsResponse.data : [];\r\n    }\r\n\r\n    // Fetch recent disaster updates (take first 3)\r\n    const updatesResponse = await residentUpdatesService.getDisasterUpdates();\r\n    if (updatesResponse && updatesResponse.status === 200 && updatesResponse.data) {\r\n      dashboardData.recent_disaster_updates = Array.isArray(updatesResponse.data) ? updatesResponse.data.slice(0, 3) : [];\r\n    }\r\n\r\n    // Fetch active predictions (take first 3)\r\n    const predictionsResponse = await residentPredictionsService.getDisasterPredictions();\r\n    if (predictionsResponse && predictionsResponse.status === 200 && predictionsResponse.data) {\r\n      dashboardData.active_predictions = Array.isArray(predictionsResponse.data) ? predictionsResponse.data.slice(0, 3) : [];\r\n    }\r\n  } catch (error) {\r\n    const status = error.response?.status;\r\n    if (status === 404) {\r\n      errorMessage.value = 'Dashboard endpoint not found (404). Ensure System-E is running on port 8001 and exposes /api/dashboard.';\r\n    } else if (status === 0 || !status) {\r\n      errorMessage.value = 'Unable to reach System-E API. Is the backend running at http://localhost:8001?';\r\n    } else {\r\n      errorMessage.value = error.response?.data?.message || 'Failed to load dashboard data';\r\n    }\r\n    console.error('Dashboard error:', error);\r\n  } finally {\r\n    loading.value = false;\r\n  }\r\n};\r\n\r\nconst useMockData = () => {\r\n  Object.assign(dashboardData, {\r\n    total_evacuation_areas: 10,\r\n    families_evacuated: 25,\r\n    total_people: 85,\r\n    available_areas: 7,\r\n    full_areas: 3,\r\n    emergency_alerts: [\r\n      { id: 1, title: 'Typhoon Warning', description: 'Severe typhoon expected.', severity: 'high', created_at: new Date().toISOString() },\r\n      { id: 2, title: 'Flash Flood Alert', description: 'Heavy rainfall may cause flooding.', severity: 'medium', created_at: new Date().toISOString() }\r\n    ],\r\n    recent_disaster_updates: [\r\n      { id: 1, title: 'Typhoon Update', description: 'Typhoon approaching.', created_at: new Date().toISOString() },\r\n      { id: 2, title: 'Flood Update', description: 'Flood levels rising.', created_at: new Date().toISOString() },\r\n      { id: 3, title: 'Earthquake Update', description: 'Aftershocks expected.', created_at: new Date().toISOString() }\r\n    ],\r\n    active_predictions: [\r\n      { id: 1, title: 'Typhoon Prediction', description: 'High risk in 24 hours.', created_at: ne