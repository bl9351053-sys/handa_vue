{"ast":null,"code":"import \"core-js/modules/esnext.json.parse.js\";\nimport { reactive, ref, onMounted } from 'vue';\nimport { residentDashboardService, residentEmergencyAlertsService, residentUpdatesService, residentPredictionsService } from '../../services/residentService';\nexport default {\n  __name: 'ResidentDashboardPage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const loading = ref(false);\n    const errorMessage = ref('');\n    const userDisplayName = ref('User');\n    const dashboardData = reactive({\n      total_evacuation_areas: 0,\n      families_evacuated: 0,\n      total_people: 0,\n      available_areas: 0,\n      full_areas: 0,\n      emergency_alerts: [],\n      recent_disaster_updates: [],\n      active_predictions: []\n    });\n    onMounted(() => {\n      const user = JSON.parse(localStorage.getItem('resident_user') || '{}');\n      userDisplayName.value = user.name || 'Resident';\n      fetchDashboard();\n    });\n    const fetchDashboard = async () => {\n      loading.value = true;\n      errorMessage.value = '';\n      try {\n        // Fetch dashboard stats\n        const dashboardResponse = await residentDashboardService.getDashboard();\n        if (dashboardResponse && dashboardResponse.status === 200 && dashboardResponse.data && dashboardResponse.data.stats) {\n          const stats = dashboardResponse.data.stats;\n          dashboardData.total_evacuation_areas = stats.total_evacuation_areas || 0;\n          dashboardData.families_evacuated = stats.total_families || 0;\n          dashboardData.total_people = stats.total_members || 0;\n          dashboardData.available_areas = stats.available_evacuation_areas || 0;\n          dashboardData.full_areas = (stats.total_evacuation_areas || 0) - (stats.available_evacuation_areas || 0);\n        } else {\n          errorMessage.value = 'Dashboard endpoint returned no data. Check System-E API at http://localhost:8001/api/dashboard.';\n          console.warn('Dashboard returned empty:', dashboardResponse);\n        }\n\n        // Fetch emergency alerts\n        const alertsResponse = await residentEmergencyAlertsService.getEmergencyAlerts();\n        if (alertsResponse && alertsResponse.status === 200 && alertsResponse.data) {\n          dashboardData.emergency_alerts = Array.isArray(alertsResponse.data) ? alertsResponse.data : [];\n        }\n\n        // Fetch recent disaster updates (take first 3)\n        const updatesResponse = await residentUpdatesService.getDisasterUpdates();\n        if (updatesResponse && updatesResponse.status === 200 && updatesResponse.data) {\n          dashboardData.recent_disaster_updates = Array.isArray(updatesResponse.data) ? updatesResponse.data.slice(0, 3) : [];\n        }\n\n        // Fetch active predictions (take first 3)\n        const predictionsResponse = await residentPredictionsService.getDisasterPredictions();\n        if (predictionsResponse && predictionsResponse.status === 200 && predictionsResponse.data) {\n          dashboardData.active_predictions = Array.isArray(predictionsResponse.data) ? predictionsResponse.data.slice(0, 3) : [];\n        }\n      } catch (error) {\n        const status = error.response?.status;\n        if (status === 404) {\n          errorMessage.value = 'Dashboard endpoint not found (404). Ensure System-E is running on port 8001 and exposes /api/dashboard.';\n        } else if (status === 0 || !status) {\n          errorMessage.value = 'Unable to reach System-E API. Is the backend running at http://localhost:8001?';\n        } else {\n          errorMessage.value = error.response?.data?.message || 'Failed to load dashboard data';\n        }\n        console.error('Dashboard error:', error);\n      } finally {\n        loading.value = false;\n      }\n    };\n    const useMockData = () => {\n      Object.assign(dashboardData, {\n        total_evacuation_areas: 10,\n        families_evacuated: 25,\n        total_people: 85,\n        available_areas: 7,\n        full_areas: 3,\n        emergency_alerts: [{\n          id: 1,\n          title: 'Typhoon Warning',\n          description: 'Severe typhoon expected.',\n          severity: 'high',\n          created_at: new Date().toISOString()\n        }, {\n          id: 2,\n          title: 'Flash Flood Alert',\n          description: 'Heavy rainfall may cause flooding.',\n          severity: 'medium',\n          created_at: new Date().toISOString()\n        }],\n        recent_disaster_updates: [{\n          id: 1,\n          title: 'Typhoon Update',\n          description: 'Typhoon approaching.',\n          created_at: new Date().toISOString()\n        }, {\n          id: 2,\n          title: 'Flood Update',\n          description: 'Flood levels rising.',\n          created_at: new Date().toISOString()\n        }, {\n          id: 3,\n          title: 'Earthquake Update',\n          description: 'Aftershocks expected.',\n          created_at: new Date().toISOString()\n        }],\n        active_predictions: [{\n          id: 1,\n          title: 'Typhoon Prediction',\n          description: 'High risk in 24 hours.',\n          created_at: new Date().toISOString()\n        }, {\n          id: 2,\n          title: 'Flood Prediction',\n          description: 'Flood risk increasing.',\n          created_at: new Date().toISOString()\n        }, {\n          id: 3,\n          title: 'Earthquake Prediction',\n          description: 'Seismic activity detected.',\n          created_at: new Date().toISOString()\n        }]\n      });\n      errorMessage.value = '';\n    };\n    const formatDate = dateString => {\n      const date = new Date(dateString);\n      return date.toLocaleDateString() + ' ' + date.toLocaleTimeString([], {\n        hour: '2-digit',\n        minute: '2-digit'\n      });\n    };\n    const __returned__ = {\n      loading,\n      errorMessage,\n      userDisplayName,\n      dashboardData,\n      fetchDashboard,\n      useMockData,\n      formatDate,\n      reactive,\n      ref,\n      onMounted,\n      get residentDashboardService() {\n        return residentDashboardService;\n      },\n      get residentEmergencyAlertsService() {\n        return residentEmergencyAlertsService;\n      },\n      get residentUpdatesService() {\n        return residentUpdatesService;\n      },\n      get residentPredictionsService() {\n        return residentPredictionsService;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["reactive","ref","onMounted","residentDashboardService","residentEmergencyAlertsService","residentUpdatesService","residentPredictionsService","loading","errorMessage","userDisplayName","dashboardData","total_evacuation_areas","families_evacuated","total_people","available_areas","full_areas","emergency_alerts","recent_disaster_updates","active_predictions","user","JSON","parse","localStorage","getItem","value","name","fetchDashboard","dashboardResponse","getDashboard","status","data","stats","total_families","total_members","available_evacuation_areas","console","warn","alertsResponse","getEmergencyAlerts","Array","isArray","updatesResponse","getDisasterUpdates","slice","predictionsResponse","getDisasterPredictions","error","response","message","useMockData","Object","assign","id","title","description","severity","created_at","Date","toISOString","formatDate","dateString","date","toLocaleDateString","toLocaleTimeString","hour","minute"],"sources":["C:/VUEprojects/crud-app/src/pages/resident_pages/ResidentDashboardPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard-page\">\r\n    <div class=\"dashboard-header\">\r\n      <h1>Dashboard</h1>\r\n      <p>Welcome, {{ userDisplayName }}!</p>\r\n    </div>\r\n\r\n    <div v-if=\"loading\" class=\"loading-state\">\r\n      <div class=\"spinner-large\"></div>\r\n      <p>Loading dashboard...</p>\r\n    </div>\r\n\r\n    <div v-else-if=\"errorMessage\" class=\"error-state\">\r\n      <div class=\"alert alert-error\">\r\n        <strong>Error:</strong> {{ errorMessage }}\r\n      </div>\r\n      <div class=\"error-actions\" style=\"display:flex;gap:8px;justify-content:center;margin-top:12px;\">\r\n        <button @click=\"fetchDashboard\" class=\"btn btn-secondary\">Retry</button>\r\n        <button @click=\"useMockData\" class=\"btn btn-secondary\">Use sample data</button>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else class=\"dashboard-content\">\r\n      <!-- Quick Stats -->\r\n      <div class=\"stats-grid\">\r\n        <div class=\"stat-card\">\r\n          <div class=\"stat-icon alert-icon\">üè†</div>\r\n          <div class=\"stat-info\">\r\n            <p class=\"stat-label\">Total Evacuation Areas</p>\r\n            <p class=\"stat-value\">{{ dashboardData.total_evacuation_areas || 0 }}</p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"stat-card\">\r\n          <div class=\"stat-icon family-icon\">üë®‚Äçüë©‚Äçüëß‚Äçüë¶</div>\r\n          <div class=\"stat-info\">\r\n            <p class=\"stat-label\">Families Evacuated</p>\r\n            <p class=\"stat-value\">{{ dashboardData.families_evacuated || 0 }}</p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"stat-card\">\r\n          <div class=\"stat-icon location-icon\">üë•</div>\r\n          <div class=\"stat-info\">\r\n            <p class=\"stat-label\">Total People</p>\r\n            <p class=\"stat-value\">{{ dashboardData.total_people || 0 }}</p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"stat-card\">\r\n          <div class=\"stat-icon facility-icon\">‚úÖ</div>\r\n          <div class=\"stat-info\">\r\n            <p class=\"stat-label\">Available Areas</p>\r\n            <p class=\"stat-value\">{{ dashboardData.available_areas || 0 }}</p>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"stat-card\">\r\n          <div class=\"stat-icon alert-icon\">‚ùå</div>\r\n          <div class=\"stat-info\">\r\n            <p class=\"stat-label\">Full Areas</p>\r\n            <p class=\"stat-value\">{{ dashboardData.full_areas || 0 }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Emergency Alerts Section -->\r\n      <div class=\"section\">\r\n        <h2>Emergency Alerts</h2>\r\n        <div v-if=\"dashboardData.emergency_alerts && dashboardData.emergency_alerts.length > 0\" class=\"alerts-list\">\r\n          <div v-for=\"alert in dashboardData.emergency_alerts.slice(0, 3)\" :key=\"alert.id\" class=\"alert-item\">\r\n            <div class=\"alert-severity\" :class=\"`severity-${alert.severity || 'medium'}`\"></div>\r\n            <div class=\"alert-content\">\r\n              <h4>{{ alert.title }}</h4>\r\n              <p>{{ alert.description }}</p>\r\n              <span class=\"alert-time\">{{ formatDate(alert.created_at) }}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div v-else class=\"empty-state\">\r\n          <p>No emergency alerts</p>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Recent Disaster Updates Section -->\r\n      <div class=\"section\">\r\n        <h2>Recent Disaster Updates</h2>\r\n        <div v-if=\"dashboardData.recent_disaster_updates && dashboardData.recent_disaster_updates.length > 0\" class=\"updates-list\">\r\n          <div v-for=\"update in dashboardData.recent_disaster_updates\" :key=\"update.id\" class=\"update-item\">\r\n            <div class=\"update-content\">\r\n              <h4>{{ update.title }}</h4>\r\n              <p>{{ update.description }}</p>\r\n              <span class=\"update-time\">{{ formatDate(update.created_at) }}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div v-else class=\"empty-state\">\r\n          <p>No recent disaster updates</p>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Active Predictions Section -->\r\n      <div class=\"section\">\r\n        <h2>Active Predictions</h2>\r\n        <div v-if=\"dashboardData.active_predictions && dashboardData.active_predictions.length > 0\" class=\"predictions-list\">\r\n          <div v-for=\"prediction in dashboardData.active_predictions\" :key=\"prediction.id\" class=\"prediction-item\">\r\n            <div class=\"prediction-content\">\r\n              <h4>{{ prediction.title }}</h4>\r\n              <p>{{ prediction.description }}</p>\r\n              <span class=\"prediction-time\">{{ formatDate(prediction.created_at) }}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div v-else class=\"empty-state\">\r\n          <p>No active predictions</p>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Quick Actions -->\r\n      <div class=\"section\">\r\n        <h2>Quick Actions</h2>\r\n        <div class=\"actions-grid\">\r\n          <router-link to=\"/resident_pages/evacuation\" class=\"action-card\">\r\n            <div class=\"action-icon\">üö™</div>\r\n            <p>Start Evacuation</p>\r\n          </router-link>\r\n          <router-link to=\"/resident_pages/family\" class=\"action-card\">\r\n            <div class=\"action-icon\">üë•</div>\r\n            <p>Manage Family</p>\r\n          </router-link>\r\n          <router-link to=\"/resident_pages/facilities\" class=\"action-card\">\r\n            <div class=\"action-icon\">üè†</div>\r\n            <p>Find Shelter</p>\r\n          </router-link>\r\n          <router-link to=\"/resident_pages/predictions\" class=\"action-card\">\r\n            <div class=\"action-icon\">üìä</div>\r\n            <p>View Predictions</p>\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { reactive, ref, onMounted } from 'vue';\r\nimport { residentDashboardService, residentEmergencyAlertsService, residentUpdatesService, residentPredictionsService } from '../../services/residentService';\r\n\r\nconst loading = ref(false);\r\nconst errorMessage = ref('');\r\nconst userDisplayName = ref('User');\r\n\r\nconst dashboardData = reactive({\r\n  total_evacuation_areas: 0,\r\n  families_evacuated: 0,\r\n  total_people: 0,\r\n  available_areas: 0,\r\n  full_areas: 0,\r\n  emergency_alerts: [],\r\n  recent_disaster_updates: [],\r\n  active_predictions: []\r\n});\r\n\r\nonMounted(() => {\r\n  const user = JSON.parse(localStorage.getItem('resident_user') || '{}');\r\n  userDisplayName.value = user.name || 'Resident';\r\n  fetchDashboard();\r\n});\r\n\r\nconst fetchDashboard = async () => {\r\n  loading.value = true;\r\n  errorMessage.value = '';\r\n\r\n  try {\r\n    // Fetch dashboard stats\r\n    const dashboardResponse = await residentDashboardService.getDashboard();\r\n    if (dashboardResponse && dashboardResponse.status === 200 && dashboardResponse.data && dashboardResponse.data.stats) {\r\n      const stats = dashboardResponse.data.stats;\r\n      dashboardData.total_evacuation_areas = stats.total_evacuation_areas || 0;\r\n      dashboardData.families_evacuated = stats.total_families || 0;\r\n      dashboardData.total_people = stats.total_members || 0;\r\n      dashboardData.available_areas = stats.available_evacuation_areas || 0;\r\n      dashboardData.full_areas = (stats.total_evacuation_areas || 0) - (stats.available_evacuation_areas || 0);\r\n    } else {\r\n      errorMessage.value = 'Dashboard endpoint returned no data. Check System-E API at http://localhost:8001/api/dashboard.';\r\n      console.warn('Dashboard returned empty:', dashboardResponse);\r\n    }\r\n\r\n    // Fetch emergency alerts\r\n    const alertsResponse = await residentEmergencyAlertsService.getEmergencyAlerts();\r\n    if (alertsResponse && alertsResponse.status === 200 && alertsResponse.data) {\r\n      dashboardData.emergency_alerts = Array.isArray(alertsResponse.data) ? alertsResponse.data : [];\r\n    }\r\n\r\n    // Fetch recent disaster updates (take first 3)\r\n    const updatesResponse = await residentUpdatesService.getDisasterUpdates();\r\n    if (updatesResponse && updatesResponse.status === 200 && updatesResponse.data) {\r\n      dashboardData.recent_disaster_updates = Array.isArray(updatesResponse.data) ? updatesResponse.data.slice(0, 3) : [];\r\n    }\r\n\r\n    // Fetch active predictions (take first 3)\r\n    const predictionsResponse = await residentPredictionsService.getDisasterPredictions();\r\n    if (predictionsResponse && predictionsResponse.status === 200 && predictionsResponse.data) {\r\n      dashboardData.active_predictions = Array.isArray(predictionsResponse.data) ? predictionsResponse.data.slice(0, 3) : [];\r\n    }\r\n  } catch (error) {\r\n    const status = error.response?.status;\r\n    if (status === 404) {\r\n      errorMessage.value = 'Dashboard endpoint not found (404). Ensure System-E is running on port 8001 and exposes /api/dashboard.';\r\n    } else if (status === 0 || !status) {\r\n      errorMessage.value = 'Unable to reach System-E API. Is the backend running at http://localhost:8001?';\r\n    } else {\r\n      errorMessage.value = error.response?.data?.message || 'Failed to load dashboard data';\r\n    }\r\n    console.error('Dashboard error:', error);\r\n  } finally {\r\n    loading.value = false;\r\n  }\r\n};\r\n\r\nconst useMockData = () => {\r\n  Object.assign(dashboardData, {\r\n    total_evacuation_areas: 10,\r\n    families_evacuated: 25,\r\n    total_people: 85,\r\n    available_areas: 7,\r\n    full_areas: 3,\r\n    emergency_alerts: [\r\n      { id: 1, title: 'Typhoon Warning', description: 'Severe typhoon expected.', severity: 'high', created_at: new Date().toISOString() },\r\n      { id: 2, title: 'Flash Flood Alert', description: 'Heavy rainfall may cause flooding.', severity: 'medium', created_at: new Date().toISOString() }\r\n    ],\r\n    recent_disaster_updates: [\r\n      { id: 1, title: 'Typhoon Update', description: 'Typhoon approaching.', created_at: new Date().toISOString() },\r\