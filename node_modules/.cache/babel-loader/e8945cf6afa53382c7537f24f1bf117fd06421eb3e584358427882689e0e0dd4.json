actual));\n      const forecast = data.map(d => Number(d.forecast));\n      new Chart(ctx, {\n        type: 'line',\n        data: {\n          labels,\n          datasets: [{\n            label: 'Actual',\n            data: actual,\n            borderColor: '#198754',\n            backgroundColor: 'rgba(25,135,84,0.1)',\n            spanGaps: true\n          }, {\n            label: 'Forecast',\n            data: forecast,\n            borderColor: '#0d6efd',\n            backgroundColor: 'rgba(13,110,253,0.08)'\n          }]\n        },\n        options: {\n          scales: {\n            x: {\n              display: true\n            },\n            y: {\n              display: true,\n              beginAtZero: true\n            }\n          }\n        }\n      });\n    }\n    onMounted(() => {\n      loadDashboard();\n    });\n    const __returned__ = {\n      stats,\n      isDownloading,\n      loadDashboard,\n      downloadBackup,\n      buildEvacuationChart,\n      buildFacilityChart,\n      buildFamilyChart,\n      buildForecastChart,\n      ref,\n      onMounted,\n      get api() {\n        return api;\n      },\n      get Chart() {\n        return Chart;\n      },\n      get registerables() {\n        return registerables;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","api","Chart","registerables","register","stats","isDownloading","loadDashboard","res","get","data","body","value","evacuationAreaData","buildEvacuationChart","facilityStatusData","buildFacilityChart","familyStatusData","buildFamilyChart","forecastData","buildForecastChart","latestForecast","undefined","latest_forecast","e","console","error","downloadBackup","createRes","post","filename","downloadUrl","defaults","baseURL","link","document","createElement","href","download","appendChild","click","removeChild","alert","ctx","ge