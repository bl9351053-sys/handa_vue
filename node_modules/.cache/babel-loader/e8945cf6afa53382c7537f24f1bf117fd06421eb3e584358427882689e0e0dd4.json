{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { ref, onMounted } from 'vue';\nimport api from '../services/api';\nimport { Chart, registerables } from 'chart.js';\nexport default {\n  __name: 'DashboardPage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    Chart.register(...registerables);\n    const stats = ref({});\n    const isDownloading = ref(false);\n    async function loadDashboard() {\n      try {\n        const res = await api.get('/dashboard');\n        if (res && res.data) {\n          const body = res.data;\n          if (body.stats) stats.value = body.stats;\n          if (body.evacuationAreaData) buildEvacuationChart(body.evacuationAreaData);\n          if (body.facilityStatusData) buildFacilityChart(body.facilityStatusData);\n          if (body.familyStatusData) buildFamilyChart(body.familyStatusData);\n          if (body.forecastData) buildForecastChart(body.forecastData);\n          // expose latestForecast into stats for simple display\n          if (body.latestForecast !== undefined) stats.value.latest_forecast = body.latestForecast;\n        }\n      } catch (e) {\n        console.error('dashboard load failed', e);\n      }\n    }\n    async function downloadBackup() {\n      isDownloading.value = true;\n      try {\n        // First create backup\n        const createRes = await api.post('/backup');\n        if (createRes && createRes.data && createRes.data.filename) {\n          const filename = createRes.data.filename;\n          // Then download the file\n          const downloadUrl = `${api.defaults.baseURL}/backup/download/${filename}`;\n          const link = document.createElement('a');\n          link.href = downloadUrl;\n          link.download = filename;\n          document.body.appendChild(link);\n          link.click();\n          document.body.removeChild(link);\n        }\n      } catch (e) {\n        console.error('backup 