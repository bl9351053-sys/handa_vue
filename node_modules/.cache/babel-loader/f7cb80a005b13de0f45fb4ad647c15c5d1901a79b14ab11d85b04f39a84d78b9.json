ocument.head.appendChild(link);\n          }\n          if (!document.querySelector(`script[src=\"${jsSrc}\"]`)) {\n            const script = document.createElement('script');\n            script.src = jsSrc;\n            script.async = true;\n            script.onload = () => resolve(window.L);\n            script.onerror = () => reject(new Error('Failed to load Leaflet'));\n            document.head.appendChild(script);\n            setTimeout(() => {\n              if (!window.L) reject(new Error('Leaflet load timed out'));\n            }, timeout);\n          } else {\n            const t = setInterval(() => {\n              if (window.L) {\n                clearInterval(t);\n                resolve(window.L);\n              }\n            }, 50);\n            setTimeout(() => {\n              clearInterval(t);\n              reject(new Error('Leaflet load timed out'));\n            }, timeout);\n          }\n        } catch (e) {\n          reject(e);\n        }\n      });\n    }\n    function waitForElement(selector, timeout = 2000) {\n      return new Promise((resolve, reject) => {\n        const el = document.querySelector(selector);\n        if (el) return resolve(el);\n        const interval = 50;\n        let elapsed = 0;\n        const t = setInterval(() => {\n          const found = document.querySelector(selector);\n          elapsed += interval;\n          if (found) {\n            clearInterval(t);\n            return resolve(found);\n          }\n          if (elapsed >= timeout) {\n            clearInterval(t);\n            return reject(new Error('Timed out waiting for ' + selector));\n          }\n        }, interval);\n      });\n    }\n    async function initLeafletSafe(lat, lng) {\n      mapReady.value = false;\n      mapError.value = null;\n      try {\n        const L = await loadLeafletAssets(10000);\n        const el = await waitForElement('#prediction-map', 5000);\n        if (!el) throw new Error('Map container not found');\n        leafletMap = L.map(el).setView([parseFloat(lat), parseFloat(lng)], 12);\n        L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {\n          attribution: '¬© OpenStreetMap contributors'\n        }).addTo(leafletMap);\n        const marker = L.marker([parseFloat(lat), parseFloat(lng)]).addTo(leafletMap);\n        marker.bindPopup(`<strong>${prediction.value.location_name || 'Location'}</strong><br>${prediction.value.disaster_type || ''}`).openPopup();\n        // Add a risk circle similar to Blade template (radius proportional to risk_level)\n        try {\n          const risk = parseInt(prediction.value.risk_level || 0, 10) || 0;\n          const color = risk >= 8 ? '#dc3545' : risk >= 6 ? '#fd7e14' : risk >= 4 ? '#ffc107' : '#28a745';\n          const radius = Math.max(500, risk * 500);\n          L.circle([parseFloat(lat), parseFloat(lng)], {\n            color: color,\n            fillColor: color,\n            fillOpacity: 0.2,\n            radius: radius\n          }).addTo(leafletMap);\n        } catch (e) {\n          console.warn('[Pred] Failed to add risk circle', e);\n        }\n        mapReady.value = true;\n      } catch (e) {\n        console.error('[Pred] Map init error', e);\n        mapError.value = e.message || String(e);\n        mapReady.value = false;\n      }\n    }\n    const __returned__ = {\n      route,\n      router,\n      residentAxios,\n      prediction,\n      loading,\n      error,\n      mapReady,\n      mapError,\n      get leafletMap() {\n        return leafletMap;\n      },\n      set leafletMap(v) {\n        leafletMap = v;\n      },\n      fetchPrediction,\n      goBack,\n      formatDate,\n      severityClass,\n      severityBgClass,\n      ucfirst,\n      relativeTime,\n      loadLeafletAssets,\n      waitForElement,\n      initLeafletSafe,\n      ref,\n      inject,\n      onMounted,\n      onUnmounted,\n      nextTick,\n      get useRoute() {\n        return useRoute;\n      },\n      get useRouter() {\n    