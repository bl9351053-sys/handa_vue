{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { ref, onMounted } from 'vue';\nimport AuthService from '../services/AuthService.js';\n\n// Backup creation\n\nexport default {\n  __name: 'BackupRecovery',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const backupLoading = ref(false);\n    const backupMessage = ref('');\n    const backupSuccess = ref(false);\n\n    // Backup list\n    const backups = ref([]);\n    const listLoading = ref(false);\n    const listError = ref('');\n\n    // Restore\n    const restoreLoading = ref('');\n    const restoreMessage = ref('');\n    const restoreSuccess = ref(false);\n\n    // Delete\n    const deleteLoading = ref('');\n    const createBackup = async () => {\n      try {\n        backupLoading.value = true;\n        backupMessage.value = '';\n        const response = await AuthService.post('/backup');\n        backupMessage.value = response.data.message;\n        backupSuccess.value = true;\n        // Refresh the backup list\n        await fetchBackups();\n      } catch (err) {\n        console.error('Failed to create backup:', err);\n        backupMessage.value = err.response?.data?.message || 'Failed to create backup. Please try again.';\n        backupSuccess.value = false;\n      } finally {\n        backupLoading.value = false;\n      }\n    };\n    const fetchBackups = async () => {\n      try {\n        listLoading.value = true;\n        listError.value = '';\n        const response = await AuthService.get('/backups');\n        backups.value = response.data.backups;\n      } catch (err) {\n        console.error('Failed to fetch backups:', err);\n        listError.value = 'Failed to load backups. Please try again.';\n      } finally {\n        listLoading.value = false;\n      }\n    };\n    const restoreBackup = async filename => {\n      if (!confirm(`Are you sure you want to restore the database from \"${filename}\"? This will overwrite the current database.`)) {\n        return;\n      }\n      try {\n        restoreLoading.value = filename;\n        restoreMessage.value = '';\n        const response = await AuthService.post('/backup/restore', {\n          filename\n        });\n        restoreMessage.value = response.data.message;\n        restoreSuccess.value = true;\n      } catch (err) {\n        console.error('Failed to restore backup:', err);\n        restoreMessage.value = err.response?.data?.message || 'Failed to restore backup. Please try again.';\n        restoreSuccess.value = false;\n      } finally {\n        restoreLoading.value = '';\n      }\n    };\n    const deleteBackup = async filename => {\n      if (!confirm(`Are you sure you want to delete the backup \"${filename}\"? This action cannot be undone.`)) {\n        return;\n      }\n      try {\n        deleteLoading.value = filename;\n        await AuthService.delete(`/backup/${filename}`);\n        // Remove from local list\n        backups.value = backups.value.filter(backup => backup.filename !== filename);\n      } catch (err) {\n        console.error('Failed to delete backup:', err);\n        alert('Failed to delete backup. Please try again.');\n      } finally {\n        deleteLoading.value = '';\n      }\n    };\n    const formatFileSize = bytes => {\n      if (bytes === 0) return '0 Bytes';\n      const k = 1024;\n      const sizes = ['Bytes', 'KB', 'MB', 'GB'];\n      const i = Math.floor(Math.log(bytes) / Math.log(k));\n      return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i];\n    };\n    const formatDateTime = dateString => {\n      if (!dateString) return 'N/A';\n      const date = new Date(dateString);\n      return date.toLocaleDateString('en-US', {\n        year: 'numeric',\n        month: 'short',\n        day: 'numeric',\n        hour: '2-digit',\n        minute: '2-digit',\n        second: '2-digit'\n      });\n    };\n    onMounted(() => {\n      fetchBackups();\n    });\n    const __returned__ = {\n      backupLoading,\n      backupMessage,\n      backupSuccess,\n      backups,\n      listLoading,\n      listError,\n      restoreLoading,\n      restoreMessage,\n      restoreSuccess,\n      deleteLoading,\n      createBackup,\n      fetchBackups,\n      restoreBackup,\n      deleteBackup,\n      formatFileSize,\n      formatDateTime,\n      ref,\n      onMounted,\n      get AuthService() {\n        return AuthService;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","AuthService","backupLoading","backupMessage","backupSuccess","backups","listLoading","listError","restoreLoading","restoreMessage","restoreSuccess","deleteLoading","createBackup","value","response","post","data","message","fetchBackups","err","console","error","get","restoreBackup","filename","confirm","deleteBackup","delete","filter","backup","alert","formatFileSize","bytes","k","sizes","i","Math","floor","log","parseFloat","pow","toFixed","formatDateTime","dateString","date","Date","toLocaleDateString","year","month","day","hour","minute","second"],"sources":["C:/VUEprojects/crud-app/src/pages/BackupRecovery.vue"],"sourcesContent":["<template>\r\n  <div class=\"backup-recovery-page\">\r\n    <div class=\"container\">\r\n      <div class=\"header\">\r\n        <button @click=\"$router.go(-1)\" class=\"back-btn\">\r\n          <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\r\n            <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M15 19l-7-7 7-7\" />\r\n          </svg>\r\n          Back to Dashboard\r\n        </button>\r\n        <h1 class=\"page-title\">Database Backup & Recovery</h1>\r\n      </div>\r\n\r\n      <!-- Backup Creation Section -->\r\n      <div class=\"backup-section\">\r\n        <div class=\"section-header\">\r\n          <h2>Create Database Backup</h2>\r\n          <p>Create a full backup of the database for safekeeping.</p>\r\n        </div>\r\n\r\n        <div class=\"backup-actions\">\r\n          <button\r\n            @click=\"createBackup\"\r\n            :disabled=\"backupLoading\"\r\n            class=\"primary-btn\"\r\n          >\r\n            <svg v-if=\"backupLoading\" class=\"spinner\" xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\r\n              <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M4 4v5h.582m15.356 2A8.001 8.001 0 004.582 9m0 0H9m11 11v-5h-.581m0 0a8.003 8.003 0 01-15.357-2m15.357 2H15\" />\r\n            </svg>\r\n            <svg v-else xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\r\n              <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M8 7H5a2 2 0 00-2 2v9a2 2 0 002 2h14a2 2 0 002-2V9a2 2 0 00-2-2h-3m-1 4l-3 3m0 0l-3-3m3 3V4\" />\r\n            </svg>\r\n            {{ backupLoading ? 'Creating Backup...' : 'Create Backup' }}\r\n          </button>\r\n        </div>\r\n\r\n        <div v-if=\"backupMessage\" class=\"message\" :class=\"{ 'success': backupSuccess, 'error': !backupSuccess }\">\r\n          {{ backupMessage }}\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Backup List Section -->\r\n      <div class=\"backup-list-section\">\r\n        <div class=\"section-header\">\r\n          <h2>Available Backups</h2>\r\n          <p>Manage your database backup files.</p>\r\n        </div>\r\n\r\n        <div v-if=\"listLoading\" class=\"loading\">\r\n          <div class=\"spinner\"></div>\r\n          <p>Loading backups...</p>\r\n        </div>\r\n\r\n        <div v-else-if=\"listError\" class=\"error\">\r\n          <p>{{ listError }}</p>\r\n          <button @click=\"fetchBackups\" class=\"retry-btn\">Retry</button>\r\n        </div>\r\n\r\n        <div v-else-if=\"backups.length === 0\" class=\"empty-state\">\r\n          <div class=\"empty-icon\">\r\n            <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\r\n              <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M5 8h14M5 8a2 2 0 110-4h14a2 2 0 110 4M5 8v10a2 2 0 002 2h10a2 2 0 002-2V8m-9 4h4\" />\r\n            </svg>\r\n          </div>\r\n          <h3>No Backups Found</h3>\r\n          <p>Create your first backup to get started.</p>\r\n        </div>\r\n\r\n        <div v-else class=\"backup-list\">\r\n          <div class=\"backup-header\">\r\n            <h3>Backup Files ({{ backups.length }})</h3>\r\n            <button @click=\"fetchBackups\" class=\"refresh-btn\">\r\n              <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\r\n                <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M4 4v5h.582m15.356 2A8.001 8.001 0 004.582 9m0 0H9m11 11v-5h-.581m0 0a8.003 8.003 0 01-15.357-2m15.357 2H15\" />\r\n              </svg>\r\n              Refresh\r\n            </button>\r\n          </div>\r\n\r\n          <div class=\"backup-items\">\r\n            <div\r\n              v-for=\"backup in backups\"\r\n              :key=\"backup.filename\"\r\n              class=\"backup-item\"\r\n            >\r\n              <div class=\"backup-icon\">\r\n                <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\r\n                  <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z\" />\r\n                </svg>\r\n              </div>\r\n\r\n              <div class=\"backup-details\">\r\n                <div class=\"backup-filename\">{{ backup.filename }}</div>\r\n                <div class=\"backup-meta\">\r\n                  <span class=\"backup-size\">{{ formatFileSize(backup.size) }}</span>\r\n                  <span class=\"backup-date\">{{ formatDateTime(backup.created_at) }}</span>\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"backup-actions\">\r\n                <button\r\n                  @click=\"restoreBackup(backup.filename)\"\r\n                  :disabled=\"restoreLoading === backup.filename\"\r\n                  class=\"restore-btn\"\r\n                >\r\n                  <svg v-if=\"restoreLoading === backup.filename\" class=\"spinner\" xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\r\n                    <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M4 4v5h.582m15.356 2A8.001 8.001 0 004.582 9m0 0H9m11 11v-5h-.581m0 0a8.003 8.003 0 01-15.357-2m15.357 2H15\" />\r\n                  </svg>\r\n                  <svg v-else xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\r\n                    <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M4 16v1a3 3 0 003 3h10a3 3 0 003-3v-1m-4-4l-4 4m0 0l-4-4m4 4V4\" />\r\n                  </svg>\r\n                  Restore\r\n                </button>\r\n\r\n                <button\r\n                  @click=\"deleteBackup(backup.filename)\"\r\n                  :disabled=\"deleteLoading === backup.filename\"\r\n                  class=\"delete-btn\"\r\n                >\r\n                  <svg v-if=\"deleteLoading === backup.filename\" class=\"spinner\" xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\r\n                    <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M4 4v5h.582m15.356 2A8.001 8.001 0 004.582 9m0 0H9m11 11v-5h-.581m0 0a8.003 8.003 0 01-15.357-2m15.357 2H15\" />\r\n                  </svg>\r\n                  <svg v-else xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\r\n                    <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M19 7l-.867 12.142A2 2 0 0116.138 21H7.862a2 2 0 01-1.995-1.858L5 7m5 4v6m4-6v6m1-10V4a1 1 0 00-1-1h-4a1 1 0 00-1 1v3M4 7h16\" />\r\n                  </svg>\r\n                  Delete\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div v-if=\"restoreMessage\" class=\"message\" :class=\"{ 'success': restoreSuccess, 'error': !restoreSuccess }\">\r\n          {{ restoreMessage }}\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from 'vue'\r\nimport AuthService from '../services/AuthService.js'\r\n\r\n// Backup creation\r\nconst backupLoading = ref(false)\r\nconst backupMessage = ref('')\r\nconst backupSuccess = ref(false)\r\n\r\n// Backup list\r\nconst backups = ref([])\r\nconst listLoading = ref(false)\r\nconst listError = ref('')\r\n\r\n// Restore\r\nconst restoreLoading = ref('')\r\nconst restoreMessage = ref('')\r\nconst restoreSuccess = ref(false)\r\n\r\n// Delete\r\nconst deleteLoading = ref('')\r\n\r\nconst createBackup = async () => {\r\n  try {\r\n    backupLoading.value = true\r\n    backupMessage.value = ''\r\n    const response = await AuthService.post('/backup')\r\n    backupMessage.value = response.data.message\r\n    backupSuccess.value = true\r\n    // Refresh the backup list\r\n    await fetchBackups()\r\n  } catch (err) {\r\n    console.error('Failed to create backup:', err)\r\n    backupMessage.value = err.response?.data?.message || 'Failed to create backup. Please try again.'\r\n    backupSuccess.value = false\r\n  } finally {\r\n    backupLoading.value = false\r\n  }\r\n}\r\n\r\nconst fetchBackups = async () => {\r\n  try {\r\n    listLoading.value = true\r\n    listError.value = ''\r\n    const response = await AuthService.get('/backups')\r\n    backups.value = response.data.backups\r\n  } catch (err) {\r\n    console.error('Failed to fetch backups:', err)\r\n    listError.value = 'Failed to load backups. Please try again.'\r\n  } finally {\r\n    listLoading.value = false\r\n  }\r\n}\r\n\r\nconst restoreBackup = async (filename) => {\r\n  if (!confirm(`Are you sure you want to restore the database from \"${filename}\"? This will overwrite the current database.`)) {\r\n    return\r\n  }\r\n\r\n  try {\r\n    restoreLoading.value = filename\r\n    restoreMessage.value = ''\r\n    const response = await AuthService.post('/backup/restore', { filename })\r\n    restoreMessage.value = response.data.message\r\n    restoreSuccess.value = true\r\n  } catch (err) {\r\n    console.error('Failed to restore backup:', err)\r\n    restoreMessage.value = err.response?.data?.message || 'Failed to restore backup. Please try again.'\r\n    restoreSuccess.value = false\r\n  } finally {\r\n    restoreLoading.value = ''\r\n  }\r\n}\r\n\r\nconst deleteBackup = async (filename) => {\r\n  if (!confirm(`Are you sure you want to delete the backup \"${filename}\"? This action cannot be undone.`)) {\r\n    return\r\n  }\r\n\r\n  try {\r\n    deleteLoading.value = filename\r\n    await AuthService.delete(`/backup/${filename}`)\r\n    // Remove from local list\r\n    backups.value = backups.value.filter(backup => backup.filename !== filename)\r\n  } catch (err) {\r\n    console.error('Failed to delete backup:', err)\r\n    alert('Failed to delete backup. Please try again.')\r\n  } finally {\r\n    deleteLoading.value = ''\r\n  }\r\n}\r\n\r\nconst formatFileSize = (bytes) => {\r\n  if (bytes === 0) return '0 Bytes'\r\n  const k = 1024\r\n  const sizes = ['Bytes', 'KB', 'MB', 'GB']\r\n  const i = Math.floor(Math.log(bytes) / Math.log(k))\r\n  return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i]\r\n}\r\n\r\nconst formatDateTime = (dateString) => {\r\n  if (!dateString) return 'N/A'\r\n  const date = new Date(dateString)\r\n  return date.toLocaleDateString('en-US', {\r\n    year: 'numeric',\r\n    month: 'short',\r\n    day: 'numeric',\r\n    hour: '2-digit',\r\n    minute: '2-digit',\r\n    second: '2-digit'\r\n  })\r\n}\r\n\r\nonMounted(() => {\r\n  fetchBackups()\r\n})\r\n</script>\r\n\r\n<style scoped>\r\n.backup-recovery-page {\r\n  padding: 20px;\r\n  background-color: #f8f9fa;\r\n  min-height: 100vh;\r\n}\r\n\r\n.container {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.header {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 20px;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.back-btn {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 8px;\r\n  padding: 10px 16px;\r\n  background: white;\r\n  border: 1px solid #e5e7eb;\r\n  border-radius: 8px;\r\n  color: #374151;\r\n  text-decoration: none;\r\n  font-weight: 500;\r\n  transition: all 0.2s ease;\r\n  cursor: pointer;\r\n}\r\n\r\n.back-btn:hover {\r\n  background: #f9fafb;\r\n  border-color: #d1d5db;\r\n}\r\n\r\n.back-btn svg {\r\n  width: 16px;\r\n  height: 16px;\r\n}\r\n\r\n.page-title {\r\n  color: #333;\r\n  font-size: 2rem;\r\n  font-weight: bold;\r\n  margin: 0;\r\n}\r\n\r\n.backup-section,\r\n.backup-list-section {\r\n  background: white;\r\n  border-radius: 12px;\r\n  padding: 24px;\r\n  margin-bottom: 24px;\r\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.section-header {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.section-header h2 {\r\n  color: #1f2937;\r\n  font-size: 1.5rem;\r\n  font-weight: 600;\r\n  margin: 0 0 8px 0;\r\n}\r\n\r\n.section-header p {\r\n  color: #6b7280;\r\n  margin: 0;\r\n}\r\n\r\n.primary-btn {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 8px;\r\n  padding: 12px 24px;\r\n  background: linear-gradient(135deg, #4f46e5, #6366f1);\r\n  color: white;\r\n  border: none;\r\n  border-radius: 8px;\r\n  font-weight: 500;\r\n  cursor: poi