valid': errors.name }\"\r\n                required\r\n              >\r\n              <small v-if=\"errors.name\" class=\"text-danger\">{{ errors.name }}</small>\r\n            </div>\r\n            <div class=\"col-md-6 mb-3\">\r\n              <label for=\"status\" class=\"form-label\">Status *</label>\r\n              <select\r\n                id=\"status\"\r\n                v-model=\"form.status\"\r\n                class=\"form-select\"\r\n                :class=\"{ 'is-invalid': errors.status }\"\r\n                required\r\n              >\r\n                <option value=\"\">Select Status</option>\r\n                <option value=\"available\">Available</option>\r\n                <option value=\"full\">Full</option>\r\n                <option value=\"closed\">Closed</option>\r\n                <option value=\"maintenance\">Maintenance</option>\r\n              </select>\r\n              <small v-if=\"errors.status\" class=\"text-danger\">{{ errors.status }}</small>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Address -->\r\n          <div class=\"mb-3\">\r\n            <label for=\"address\" class=\"form-label\">Address *</label>\r\n            <input\r\n              id=\"address\"\r\n              v-model=\"form.address\"\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              :class=\"{ 'is-invalid': errors.address }\"\r\n              required\r\n            >\r\n            <small v-if=\"errors.address\" class=\"text-danger\">{{ errors.address }}</small>\r\n          </div>\r\n\r\n          <!-- Capacity -->\r\n          <div class=\"row\">\r\n            <div class=\"col-md-6 mb-3\">\r\n              <label for=\"capacity\" class=\"form-label\">Capacity *</label>\r\n              <input\r\n                id=\"capacity\"\r\n                v-model.number=\"form.capacity\"\r\n                type=\"number\"\r\n                class=\"form-control\"\r\n                :class=\"{ 'is-invalid': errors.capacity }\"\r\n                min=\"0\"\r\n                required\r\n              >\r\n              <small v-if=\"errors.capacity\" class=\"text-danger\">{{ errors.capacity }}</small>\r\n            </div>\r\n            <div class=\"col-md-6 mb-3\">\r\n              <label for=\"current_occupancy\" class=\"form-label\">Current Occupancy *</label>\r\n              <input\r\n                id=\"current_occupancy\"\r\n                v-model.number=\"form.current_occupancy\"\r\n                type=\"number\"\r\n                class=\"form-control\"\r\n                :class=\"{ 'is-invalid': errors.current_occupancy }\"\r\n                min=\"0\"\r\n                required\r\n              >\r\n              <small v-if=\"errors.current_occupancy\" class=\"text-danger\">{{ errors.current_occupancy }}</small>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Coordinates -->\r\n          <div class=\"row\">\r\n            <div class=\"col-md-6 mb-3\">\r\n              <label for=\"latitude\" class=\"form-label\">Latitude</label>\r\n              <input\r\n                id=\"latitude\"\r\n                v-model.number=\"form.latitude\"\r\n                type=\"number\"\r\n                class=\"form-control\"\r\n                step=\"0.0001\"\r\n                placeholder=\"0.0000\"\r\n                @change=\"updateMap\"\r\n              >\r\n            </div>\r\n            <div class=\"col-md-6 mb-3\">\r\n              <label for=\"longitude\" class=\"form-label\">Longitude</label>\r\n              <input\r\n                id=\"longitude\"\r\n                v-model.number=\"form.longitude\"\r\n                type=\"number\"\r\n                class=\"form-control\"\r\n                step=\"0.0001\"\r\n                placeholder=\"0.0000\"\r\n                @change=\"updateMap\"\r\n              >\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Map -->\r\n          <div class=\"mb-3\">\r\n            <label c