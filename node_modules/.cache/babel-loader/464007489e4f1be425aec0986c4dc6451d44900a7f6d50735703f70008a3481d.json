lass=\"card-body error-msg\">{{ error }}</div>\r\n    </div>\r\n\r\n    <!-- Main Content -->\r\n    <div v-else>\r\n      <!-- Area Overview Card -->\r\n      <div class=\"card\">\r\n        <div class=\"card-header\">\r\n          <div class=\"header-title\">{{ area.name || 'Evacuation Area' }}</div>\r\n          <span :class=\"['badge-custom', getStatusBadgeClass(area.status)]\">\r\n            {{ area.status }}\r\n          </span>\r\n        </div>\r\n        <div class=\"card-body\">\r\n          <div class=\"overview-layout\">\r\n            <!-- Left Column: Details -->\r\n            <div class=\"overview-left\">\r\n              <p class=\"address\">üìç {{ area.address }}</p>\r\n              \r\n              <div class=\"section\">\r\n                <h3>Capacity & Occupancy</h3>\r\n                <div class=\"info-grid\">\r\n                  <div class=\"info-item\">\r\n                    <span class=\"label\">Total Capacity</span>\r\n                    <span class=\"value\">{{ area.capacity ?? '‚Äî' }} persons</span>\r\n                  </div>\r\n                  <div class=\"info-item\">\r\n                    <span class=\"label\">Current Occupancy</span>\r\n                    <span class=\"value\">{{ area.current_occupancy ?? 0 }} persons</span>\r\n                  </div>\r\n                  <div class=\"info-item\">\r\n                    <span class=\"label\">Available Spaces</span>\r\n                    <span class=\"value\" :style=\"getCapacityColor()\">\r\n                      {{ (area.capacity - (area.current_occupancy ?? 0)) }} persons\r\n                    </span>\r\n                  </div>\r\n                </div>\r\n                <div class=\"capacity-bar\">\r\n                  <div class=\"capacity-fill\" :style=\"{ width: getCapacityPercent() + '%' }\"></div>\r\n                </div>\r\n                <div class=\"capacity-text\">\r\n                  {{ getCapacityPercent().toFixed(0) }}% occupied\r\n                