{"ast":null,"code":"import { ref } from 'vue';\nimport { inject } from 'vue';\nexport default {\n  __name: 'RecommendArea',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const residentAxios = inject('residentAxios', null);\n    let axiosFallback = null;\n    const geoSupported = 'geolocation' in navigator;\n    const loading = ref(false);\n    const error = ref('');\n    const recommended = ref(null);\n    const ranked = ref([]);\n\n    /**\r\n     * Get recommendations based on user's current location\r\n     */\n    async function getRecommendations() {\n      if (!geoSupported) {\n        error.value = 'Geolocation not available';\n        return;\n      }\n      loading.value = true;\n      error.value = '';\n      recommended.value = null;\n      ranked.value = [];\n      navigator.geolocation.getCurrentPosition(async pos => {\n        const lat = pos.coords.latitude;\n        const lng = pos.coords.longitude;\n        console.debug('[RecommendArea] got location', {\n          lat,\n          lng\n        });\n        try {\n          if (!axiosFallback) {\n            const mod = await import('axios');\n            axiosFallback = mod.default || mod;\n          }\n          const http = residentAxios || axiosFallback;\n\n          // API expects latitude & longitude as query params\n          const res = await http.get('/evacuation-areas/recommend', {\n            params: {\n              latitude: lat,\n              longitude: lng\n            }\n          });\n          const payload = res.data || {};\n          recommended.value = payload.recommended_area ?? payload.recommended ?? null;\n          ranked.value = payload.ranked_areas ?? payload.ranked ?? payload.rankings ?? [];\n          console.debug('[RecommendArea] got recommendations', {\n            recommended: recommended.value,\n            rankedCount: ranked.value.length\n          });\n        } catch (e) {\n          console.error('[RecommendArea] recommendation error', e);\n          error.value = e.response?.data?.message || e.message || 'Failed to get recommendations';\n        } finally {\n          loading.value = false;\n        }\n      }, err => {\n        loading.value = false;\n        error.value = err.message || 'Geolocation error';\n        console.error('[RecommendArea] geolocation error', err);\n      });\n    }\n    const __returned__ = {\n      residentAxios,\n      get axiosFallback() {\n        return axiosFallback;\n      },\n      set axiosFallback(v) {\n        axiosFallback = v;\n      },\n      geoSupported,\n      loading,\n      error,\n      recommended,\n      ranked,\n      getRecommendations,\n      ref,\n      inject\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","inject","residentAxios","axiosFallback","geoSupported","navigator","loading","error","recommended","ranked","getRecommendations","value","geolocation","getCurrentPosition","pos","lat","coords","latitude","lng","longitude","console","debug","mod","default","http","res","get","params","payload","data","recommended_area","ranked_areas","rankings","rankedCount","length","e","response","message","err"],"sources":["C:/VUEprojects/crud-app/src/pages/resident_pages/RecommendArea.vue"],"sourcesContent":["<template>\r\n  <div class=\"recommend-area\">\r\n    <div class=\"header-controls\">\r\n      <router-link to=\"/resident_pages/evacuation-areas\" class=\"btn btn-secondary\">‚Üê Back to Areas</router-link>\r\n    </div>\r\n\r\n    <div class=\"card\">\r\n      <div class=\"card-header\">üéØ Find Nearest Evacuation Area</div>\r\n      <div class=\"card-body\">\r\n        <!-- Geolocation Not Supported -->\r\n        <div v-if=\"!geoSupported\" class=\"alert alert-warning\">\r\n          ‚ö†Ô∏è Geolocation is not supported in your browser. Please enable location services or use a modern browser.\r\n        </div>\r\n\r\n        <div v-else>\r\n          <!-- Get Recommendations Button -->\r\n          <div class=\"controls\">\r\n            <p class=\"info-text\">\r\n              We'll use your current location to find the nearest and best evacuation areas for you.\r\n            </p>\r\n            <button \r\n              class=\"btn btn-primary btn-large\" \r\n              @click=\"getRecommendations\" \r\n              :disabled=\"loading\"\r\n            >\r\n              üìç {{ loading ? 'Finding Areas...' : 'Get Recommendations' }}\r\n            </button>\r\n          </div>\r\n\r\n          <!-- Loading State -->\r\n          <div v-if=\"loading\" class=\"loading-box\">\r\n            <div class=\"spinner\"></div>\r\n            <p>Finding nearby areas‚Ä¶</p>\r\n          </div>\r\n\r\n          <!-- Error State -->\r\n          <div v-if=\"error\" class=\"alert alert-danger\">\r\n            ‚ùå {{ error }}\r\n          </div>\r\n\r\n          <!-- Top Recommended Area -->\r\n          <div v-if=\"recommended\" class=\"recommended-card\">\r\n            <div class=\"recommended-header\">\r\n              <h3>‚≠ê Top Recommendation</h3>\r\n            </div>\r\n            <div class=\"area-details\">\r\n              <div class=\"area-name\">{{ recommended.name }}</div>\r\n              <div class=\"area-address\">{{ recommended.address }}</div>\r\n              <div class=\"area-info-grid\">\r\n                <div class=\"info-item\">\r\n                  <span class=\"label\">Score</span>\r\n                  <span class=\"score\">{{ (recommended.final_score ?? 0).toFixed(1) }}</span>\r\n                </div>\r\n                <div class=\"info-item\">\r\n                  <span class=\"label\">Distance</span>\r\n                  <span>{{ (recommended.distance ?? 0).toFixed(1) }} km</span>\r\n                </div>\r\n                <div class=\"info-item\">\r\n                  <span class=\"label\">Capacity Left</span>\r\n                  <span>{{ (recommended.capacity - (recommended.current_occupancy ?? 0)) }} spaces</span>\r\n                </div>\r\n              </div>\r\n              <router-link \r\n                :to=\"`/resident_pages/evacuation-areas/${recommended.id}`\"\r\n                class=\"btn btn-primary btn-large\"\r\n              >\r\n                ‚Üí Go to Area\r\n              </router-link>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Ranked Areas List -->\r\n          <div v-if=\"ranked && ranked.length\" class=\"ranked-list\">\r\n            <h3>üîù All Ranked Areas</h3>\r\n            <div class=\"table-responsive\">\r\n              <table class=\"table table-custom\">\r\n                <thead>\r\n                  <tr>\r\n                    <th>#</th>\r\n                    <th>Area Name</th>\r\n                    <th>Distance</th>\r\n                    <th>Score</th>\r\n                    <th>Capacity Left</th>\r\n                    <th></th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"(area, idx) in ranked\" :key=\"area.id\">\r\n                    <td class=\"text-center rank\">{{ idx + 1 }}</td>\r\n                    <td>\r\n                      <div class=\"area-name\">{{ area.name }}</div>\r\n                      <div class=\"area-address\">{{ area.address }}</div>\r\n                    </td>\r\n                    <td class=\"text-center\">{{ (area.distance ?? 0).toFixed(1) }} km</td>\r\n                    <td class=\"text-center\"><strong>{{ (area.final_score ?? 0).toFixed(1) }}</strong></td>\r\n                    <td class=\"text-center\">{{ (area.capacity - (area.current_occupancy ?? 0)) }}</td>\r\n                    <td>\r\n                      <router-link \r\n                        :to=\"`/resident_pages/evacuation-areas/${area.id}`\"\r\n                        class=\"btn btn-small\"\r\n                      >\r\n                        View\r\n                      </router-link>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- No Results -->\r\n          <div v-if=\"!loading && !recommended && !ranked.length && !error\" class=\"no-results\">\r\n            <p class=\"muted\">Click the button above to find recommended evacuation areas based on your location.</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref } from 'vue';\r\nimport { inject } from 'vue';\r\n\r\nconst residentAxios = inject('residentAxios', null);\r\nlet axiosFallback = null;\r\n\r\nconst geoSupported = 'geolocation' in navigator;\r\nconst loading = ref(false);\r\nconst error = ref('');\r\nconst recommended = ref(null);\r\nconst ranked = ref([]);\r\n\r\n/**\r\n * Get recommendations based on user's current location\r\n */\r\nasync function getRecommendations() {\r\n  if (!geoSupported) {\r\n    error.value = 'Geolocation not available';\r\n    return;\r\n  }\r\n\r\n  loading.value = true;\r\n  error.value = '';\r\n  recommended.value = null;\r\n  ranked.value = [];\r\n\r\n  navigator.geolocation.getCurrentPosition(\r\n    async (pos) => {\r\n      const lat = pos.coords.latitude;\r\n      const lng = pos.coords.longitude;\r\n      \r\n      console.debug('[RecommendArea] got location', { lat, lng });\r\n\r\n      try {\r\n        if (!axiosFallback) {\r\n          const mod = await import('axios');\r\n          axiosFallback = mod.default || mod;\r\n        }\r\n        \r\n        const http = residentAxios || axiosFallback;\r\n        \r\n        // API expects latitude & longitude as query params\r\n        const res = await http.get('/evacuation-areas/recommend', {\r\n          params: { latitude: lat, longitude: lng }\r\n        });\r\n\r\n        const payload = res.data || {};\r\n        recommended.value = payload.recommended_area ?? payload.recommended ?? null;\r\n        ranked.value = payload.ranked_areas ?? payload.ranked ?? payload.rankings ?? [];\r\n\r\n        console.debug('[RecommendArea] got recommendations', {\r\n          recommended: recommended.value,\r\n          rankedCount: ranked.value.length\r\n        });\r\n      } catch (e) {\r\n        console.error('[RecommendArea] recommendation error', e);\r\n        error.value = e.response?.data?.message || e.message || 'Failed to get recommendations';\r\n      } finally {\r\n        loading.value = false;\r\n      }\r\n    },\r\n    (err) => {\r\n      loading.value = false;\r\n      error.value = err.message || 'Geolocation error';\r\n      console.error('[RecommendArea] geolocation error', err);\r\n    }\r\n  );\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.recommend-area { padding: 1rem; max-width: 800px; margin: 0 auto; }\r\n.controls { margin-bottom: 1rem; }\r\n.center { text-align:center; margin: 0.5rem 0; }\r\n.recommended-card { background:#f0f8ff; padding:10px; border-radius:8px; margin-bottom:8px; }\r\n.list ul { list-style:none; padding:0; margin:0; }\r\n.list li { padding:8px 0; border-bottom:1px solid #f0f0f0; display:flex; justify-content:space-between; }\r\n.muted { color:#666; }\r\n.btn { padding:8px 12px; border-radius:6px; color:#fff; background:#667eea; border:none; cursor:pointer; }\r\n</style>\r\n"],"mappings":"AAyHA,SAASA,GAAG,QAAQ,KAAK;AACzB,SAASC,MAAM,QAAQ,KAAK;;;;;;;IAE5B,MAAMC,aAAa,GAAGD,MAAM,CAAC,eAAe,EAAE,IAAI,CAAC;IACnD,IAAIE,aAAa,GAAG,IAAI;IAExB,MAAMC,YAAY,GAAG,aAAa,IAAIC,SAAS;IAC/C,MAAMC,OAAO,GAAGN,GAAG,CAAC,KAAK,CAAC;IAC1B,MAAMO,KAAK,GAAGP,GAAG,CAAC,EAAE,CAAC;IACrB,MAAMQ,WAAW,GAAGR,GAAG,CAAC,IAAI,CAAC;IAC7B,MAAMS,MAAM,GAAGT,GAAG,CAAC,EAAE,CAAC;;IAEtB;AACA;AACA;IACA,eAAeU,kBAAkBA,CAAA,EAAG;MAClC,IAAI,CAACN,YAAY,EAAE;QACjBG,KAAK,CAACI,KAAK,GAAG,2BAA2B;QACzC;MACF;MAEAL,OAAO,CAACK,KAAK,GAAG,IAAI;MACpBJ,KAAK,CAACI,KAAK,GAAG,EAAE;MAChBH,WAAW,CAACG,KAAK,GAAG,IAAI;MACxBF,MAAM,CAACE,KAAK,GAAG,EAAE;MAEjBN,S