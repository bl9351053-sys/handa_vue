{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport { reactive, ref, inject } from 'vue';\nimport axios from 'axios';\nimport { useRouter } from 'vue-router';\nexport default {\n  __name: 'LoginView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const baseUrl = inject('baseUrl', '');\n    const form = reactive({\n      email: '',\n      gov_id: '',\n      password: '',\n      remember: false\n    });\n    const errors = reactive({});\n    const generalError = ref('');\n    const submitting = ref(false);\n    async function submit() {\n      submitting.value = true;\n      generalError.value = '';\n      Object.keys(errors).forEach(k => delete errors[k]);\n      try {\n        // Determine payload: backend API expects a 12-digit gov_id for /api/login\n        const payload = {};\n        const emailVal = (form.email || '').toString().trim();\n        const govRaw = (form.gov_id || '').toString();\n        const govDigits = govRaw.replace(/\\D/g, '');\n        if (!emailVal && !govDigits) {\n          generalError.value = 'Please enter either Email or Government ID.';\n          submitting.value = false;\n          return;\n        }\n        if (govDigits) {\n          // enforce 12 digits expected by the API\n          if (govDigits.length !== 12) {\n            errors.gov_id = 'Government ID must be 12 digits.';\n            submitting.value = false;\n            return;\n          }\n          payload.gov_id = govDigits;\n        } else if (emailVal) {\n          // NOTE: apiLogin currently requires gov_id; if only email provided, send email (some backends accept it) but server may reject\n          payload.email = emailVal;\n        }\n        payload.password = form.password;\n        payload.remember = form.remember;\n        const res = await axios.post((baseUrl || '') + '/login', payload);\n        const token = res.data?.access_token || null;\n        if (token) {\n          localStorage.setItem('token', token);\n          axios.defaults.headers.common['Authorization'] = `Bearer ${token}`;\n        }\n        router.push({\n          name: 'Dashboard'\n        });\n      } catch (err) {\n        if (err.response && err.response.status === 422) {\n          const respErrors = err.response.data.errors || {};\n          Object.assign(errors, respErrors);\n        } else if (err.response && err.response.data && err.response.data.message) {\n          generalError.value = err.response.data.message;\n        } else {\n          generalError.value = 'Login failed. Please try again.';\n        }\n      } finally {\n        submitting.value = false;\n      }\n    }\n    const __returned__ = {\n      router,\n      baseUrl,\n      form,\n      errors,\n      generalError,\n      submitting,\n      submit,\n      reactive,\n      ref,\n      inject,\n      get axios() {\n        return axios;\n      },\n      get useRouter() {\n        return useRouter;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["reactive","ref","inject","axios","useRouter","router","baseUrl","form","email","gov_id","password","remember","errors","generalError","submitting","submit","value","Object","keys","forEach","k","payload","emailVal","toString","trim","govRaw","govDigits","replace","length","res","post","token","data","access_token","localStorage","setItem","defaults","headers","common","push","name","err","response","status","respErrors","assign","message"],"sources":["C:/VUEprojects/crud-app/src/views/LoginView.vue"],"sourcesContent":["<template>\r\n  <div class=\"min-h-screen flex items-start justify-center p-6 bg-gray-900\">\r\n    <div class=\"login-card w-full max-w-md mt-12 p-10 rounded-2xl shadow-2xl bg-white\">\r\n      <div class=\"mb-6 text-center\">\r\n        <h1 class=\"text-3xl sm:text-4xl font-bold text-gray-900 mb-2\">Sign in to Systemâ€‘E</h1>\r\n        <p class=\"text-base sm:text-lg text-gray-600\">Enter your credentials to access the admin panel</p>\r\n      </div>\r\n\r\n      <div v-if=\"generalError\" class=\"bg-red-50 border-l-4 border-red-500 text-red-800 px-4 py-3 rounded mb-6\">\r\n        <p class=\"text-sm\">{{ generalError }}</p>\r\n      </div>\r\n\r\n      <form @submit.prevent=\"submit\" class=\"space-y-6\">\r\n  <div>\r\n    <label class=\"block text-base font-medium text-gray-700 mb-2\" for=\"email\">Email (optional)</label>\r\n    <input id=\"email\" name=\"email\" type=\"email\" v-model=\"form.email\" \r\n           class=\"input-field w-full px-4 py-3 border rounded-lg text-base\" \r\n           placeholder=\"you@example.com\" />\r\n    <p v-if=\"errors.email\" class=\"text-sm text-red-600 mt-1\">{{ errors.email }}</p>\r\n  </div>\r\n\r\n  <div>\r\n    <label class=\"block text-base font-medium text-gray-700 mb-2\" for=\"gov_id\">Government ID (optional)</label>\r\n    <input id=\"gov_id\" name=\"gov_id\" v-model=\"form.gov_id\" \r\n           class=\"input-field w-full px-4 py-3 border rounded-lg text-base\" \r\n           placeholder=\"123456789012\" />\r\n    <p class=\"text-sm text-gray-500 mt-1\">Use either your Email or Government ID to sign in.</p>\r\n    <p v-if=\"errors.gov_id\" class=\"text-sm text-red-600 mt-1\">{{ errors.gov_id }}</p>\r\n  </div>\r\n\r\n  <div>\r\n    <label class=\"block text-base font-medium text-gray-700 mb-2\" for=\"password\">Password</label>\r\n    <input id=\"password\" type=\"password\" name=\"password\" v-model=\"form.password\" required \r\n           class=\"input-field w-full px-4 py-3 border rounded-lg text-base\" />\r\n    <p v-if=\"errors.password\" class=\"text-sm text-red-600 mt-1\">{{ errors.password }}</p>\r\n  </div>\r\n\r\n  <div class=\"flex flex-col items-start gap-2 mt-2\">\r\n    <label class=\"flex items-center gap-2 text-sm text-gray-700\">\r\n      <input type=\"checkbox\" id=\"remember\" v-model=\"form.remember\" class=\"w-4 h-4 text-indigo-600 border-gray-300 rounded\" />\r\n      <span>Remember me</span>\r\n    </label>\r\n    <router-link to=\"/register\" class=\"text-sm text-indigo-600 hover:underline\">Create account</router-link>\r\n  </div>\r\n\r\n  <button type=\"submit\" :disabled=\"submitting\"\r\n          class=\"w-full flex items-center justify-center gap-2 py-3 px-4 rounded-lg text-white font-medium bg-indigo-600 hover:bg-indigo-700 disabled:opacity-60 mt-4\">\r\n    <svg v-if=\"!submitting\" xmlns=\"http://www.w3.org/2000/svg\" class=\"h-5 w-5\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\r\n      <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M16 21v-2a4 4 0 00-4-4H8a4 4 0 00-4 4v2\" />\r\n      <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M12 11a4 4 0 100-8 4 4 0 000 8z\" />\r\n    </svg>\r\n    <span v-if=\"!submitting\">Sign In</span>\r\n    <svg v-else xmlns=\"http://www.w3.org/2000/svg\" class=\"animate-spin h-5 w-5\" viewBox=\"0 0 24 24\">\r\n      <circle class=\"opacity-25\" cx=\"12\" cy=\"12\" r=\"10\" stroke=\"currentColor\" stroke-width=\"4\"></circle>\r\n      <path class=\"opacity-75\" fill=\"currentColor\" d=\"M4 12a8 8 0 018-8v8z\"></path>\r\n    </svg>\r\n  </button>\r\n\r\n  <div class=\"text-center text-sm text-gray-500 mt-3\">Need help? <a href=\"#\" class=\"text-indigo-600 hover:underline\">Forgot password</a></div>\r\n</form>\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { reactive, ref, inject } from 'vue'\r\nimport axios from 'axios'\r\nimport { useRouter } from 'vue-router'\r\n\r\nconst router = useRouter()\r\nconst baseUrl = inject('baseUrl', '')\r\n\r\nconst form = reactive({\r\n  email: '',\r\n  gov_id: '',\r\n  password: '',\r\n  remember: false\r\n})\r\n\r\nconst errors = reactive({})\r\nconst generalError = ref('')\r\nconst submitting = ref(false)\r\n\r\nasync function submit() {\r\n  submitting.value = true\r\n  generalError.value = ''\r\n  Object.keys(errors).forEach(k => delete errors[k])\r\n\r\n  try {\r\n    // Determine payload: backend API expects a 12-digit gov_id for /api/login\r\n    const payload = {};\r\n    const emailVal = (form.email || '').toString().trim();\r\n    const govRaw = (form.gov_id || '').toString();\r\n    const govDigits = govRaw.replace(/\\D/g, '');\r\n\r\n    if (!emailVal && !govDigits) {\r\n      generalError.value = 'Please enter either Email or Government ID.'\r\n      submitting.value = false\r\n      return\r\n    }\r\n\r\n    if (govDigits) {\r\n      // enforce 12 digits expected by the API\r\n      if (govDigits.length !== 12) {\r\n        errors.gov_id = 'Government ID must be 12 digits.'\r\n        submitting.value = false\r\n        return\r\n      }\r\n      payload.gov_id = govDigits\r\n    } else if (emailVal) {\r\n      // NOTE: apiLogin currently requires gov_id; if only email provided, send email (some backends accept it) but server may reject\r\n      payload.email = emailVal\r\n    }\r\n    payload.password = form.password\r\n    payload.remember = form.remember\r\n\r\n    const res = await axios.post((baseUrl || '') + '/login', payload)\r\n    const token = res.data?.access_token || null\r\n    if (token) {\r\n      localStorage.setItem('token', token)\r\n      axios.defaults.headers.common['Authorization'] = `Bearer ${token}`\r\n    }\r\n\r\n    router.push({ name: 'Dashboard' })\r\n  } catch (err) {\r\n    if (err.response && err.response.status === 422) {\r\n      const respErrors = err.response.data.errors || {}\r\n      Object.assign(errors, respErrors)\r\n    } else if (err.response && err.response.data && err.response.data.message) {\r\n      generalError.value = err.response.data.message\r\n    } else {\r\n      generalError.value = 'Login failed. Please try again.'\r\n    }\r\n  } finally {\r\n    submitting.value = false\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.login-card { backdrop-filter: blur(6px); }\r\n.btn-login{ background: linear-gradient(90deg,#2563eb,#1e40af); }\r\n</style>\r\n"],"mappings":";;;AAkEA,SAASA,QAAQ,EAAEC,GAAG,EAAEC,MAAM,QAAQ,KAAK;AAC3C,OAAOC,KAAK,MAAM,OAAO;AACzB,SAASC,SAAS,QAAQ,YAAY;;;;;;;IAEtC,MAAMC,MAAM,GAAGD,SAAS,CAAC,CAAC;IAC1B,MAAME,OAAO,GAAGJ,MAAM,CAAC,SAAS,EAAE,EAAE,CAAC;IAErC,MAAMK,IAAI,GAAGP,QAAQ,CAAC;MACpBQ,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC,CAAC;IAEF,MAAMC,MAAM,GAAGZ,QAAQ,CAAC,CAAC,CAAC,CAAC;IAC3B,MAAMa,YAAY,GAAGZ,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMa,UAAU,GAAGb,GAAG,CAAC,KAAK,CAAC;IAE7B,eAAec,MAAMA,CAAA,EAAG;MACtBD,UAAU,CAACE,KAAK,GAAG,IAAI;MACvBH,YAAY,CAACG,KAAK,GAAG,EAAE;MACvBC,MAAM,CAACC,IAAI,CAACN,MAAM,CAAC,CAACO,OAAO,CAACC,CAAC,IAAI,OAAOR,MAAM,CAACQ,CAAC,CAAC,CAAC;MAElD,IAAI;QACF;QACA,MAAMC,OAAO,GAAG,CAAC,CAAC;QAClB,MAAMC,QAAQ,GAAG,CAACf,IAAI,CAACC,KAAK,IAAI,EAAE,EAAEe,QAAQ,CAAC,CAAC,CAACC,IAAI,CAAC,CAAC;QACrD,MAAMC,MAAM,GAAG,CAAClB,IAAI,CAACE,MAAM,IAAI,EAAE,EAAEc,QAAQ,CAAC,CAAC;QAC7C,MAAMG,SAAS,GAAGD,MAAM,CAACE,OAAO,CAAC,KAAK,EAAE,EAAE,CAAC;QAE3C,IAAI,CAACL,QAAQ,IAAI,CAACI,SAAS,EAAE;UAC3Bb,YAAY,CAACG,KAAK,GAAG,6CAA6C;UAClEF,UAAU,CAACE,KAAK,GAAG,KAAK;UACxB;QACF;QAEA,IAAIU,SAAS,EAAE;UACb;UACA,IAAIA,SAAS,CAACE,MAAM,KAAK,EAAE,EAAE;YAC3BhB,MAAM,CAACH,MAAM,GAAG,kCAAkC;YAClDK,UAAU,CAACE,KAAK,GAAG,KAAK;YACxB;UACF;UACAK,OAAO,CAACZ,MAAM,GAAGiB,SAAS;QAC5B,CAAC,MAAM,IAAIJ,QAAQ,EAAE;UACnB;UACAD,OAAO,CAACb,KAAK,GAAGc,QAAQ;QAC1B;QACAD,OAAO,CAACX,QAAQ,GAAGH,IAAI,CAACG,QAAQ;QAChCW,OAAO,CAACV,QAAQ,GAAGJ,IAAI,CAACI,QAAQ;QAEhC,MAAMkB,GAAG,GAAG,MAAM1B,KAAK,CAAC2B,IAAI,CAAC,CAACxB,OAAO,IAAI,EAAE,IAAI,QAAQ,EAAEe,OAAO,CAAC;QACjE,MAAMU,KAAK,GAAGF,GAAG,CAACG,IAAI,EAAEC,YAAY,IAAI,IAAI;QAC5C,IAAIF,KAAK,EAAE;UACTG,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEJ,KAAK,CAAC;UACpC5B,KAAK,CAACiC,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,eAAe,CAAC,GAAG,UAAUP,KAAK,EAAE;QACpE;QAEA1B,MAAM,CAACkC,IAAI,CAAC;UAAEC,IAAI,EA