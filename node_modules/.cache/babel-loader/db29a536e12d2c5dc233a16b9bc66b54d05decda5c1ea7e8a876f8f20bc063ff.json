{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useRoute, useRouter } from 'vue-router';\nimport { ref, onMounted } from 'vue';\nexport default {\n  __name: 'SideBar',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const router = useRouter();\n    const userName = ref('Admin User');\n    const mobileMenuOpen = ref(false);\n    const items = [{\n      name: 'dashboard',\n      label: 'Dashboard',\n      to: '/dashboard',\n      icon: 'fa fa-tachometer'\n    }, {\n      name: 'predictions',\n      label: 'Predictions',\n      to: '/predictions',\n      icon: 'fa fa-bolt'\n    }, {\n      name: 'evacuation-areas',\n      label: 'Evacuation Areas',\n      to: '/evacuation-areas',\n      icon: 'fa fa-map'\n    }, {\n      name: 'facilities',\n      label: 'Facilities',\n      to: '/facilities',\n      icon: 'fa fa-building'\n    }, {\n      name: 'families',\n      label: 'Families',\n      to: '/families',\n      icon: 'fa fa-users'\n    }, {\n      name: 'reports',\n      label: 'Reports',\n      to: '/reports',\n      icon: 'fa fa-file'\n    }];\n    function isActive(to) {\n      return route.path === to || route.path.startsWith(to + '/');\n    }\n    function logout() {\n      mobileMenuOpen.value = false;\n      localStorage.removeItem('token');\n      localStorage.removeItem('user_id');\n      // clear axios header\n      import('../services/AuthService').then(({\n        default: AuthService\n      }) => AuthService.setToken(null));\n      router.push({\n        name: 'Login'\n      });\n    }\n    onMounted(() => {\n      // Try to get user info from localStorage\n      const storedUser = localStorage.getItem('user_email') || 'Admin User';\n      userName.value = storedUser;\n    });\n    const __returned__ = {\n      route,\n      router,\n      userName,\n      mobileMenuOpen,\n      items,\n      isActive,\n      logout,\n      get useRoute() {\n        return useRoute;\n      