{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.json.parse.js\";\nimport { reactive, ref, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { residentDashboardService, residentEmergencyAlertsService, residentPredictionsService } from '../../services/residentService';\nexport default {\n  __name: 'ResidentDashboardPage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const loading = ref(false);\n    const errorMessage = ref('');\n    const userDisplayName = ref('Resident');\n    const dashboardData = reactive({\n      total_evacuation_areas: 0,\n      families_evacuated: 0,\n      total_people: 0,\n      available_areas: 0,\n      full_areas: 0,\n      emergency_alerts: [],\n      recent_disaster_updates: [],\n      active_predictions: []\n    });\n    onMounted(() => {\n      const user = JSON.parse(localStorage.getItem('resident_user') || '{}');\n      userDisplayName.value = user.name || 'Resident';\n      fetchDashboard();\n    });\n    const fetchDashboard = async () => {\n      loading.value = true;\n      errorMessage.value = '';\n      try {\n        const dashboardResponse = await residentDashboardService.getDashboard();\n        if (dashboardResponse?.status === 200) {\n          const d = dashboardResponse.data;\n          dashboardData.total_evacuation_areas = d.totalEvacuationAreas || 0;\n          dashboardData.families_evacuated = d.totalFamilies || 0;\n          dashboardData.total_people = d.totalPeople || 0;\n          dashboardData.available_areas = d.availableAreas || 0;\n          dashboardData.full_areas = d.fullAreas || 0;\n          dashboardData.recent_disaster_updates = d.recentUpdates || [];\n        }\n        const alertsResponse = await residentEmergencyAlertsService.getEmergencyAlerts();\n        if (alertsResponse?.status === 200) {\n          dashboardData.emergency_alerts = alertsResponse.data.alerts || [];\n        }\n      } catch (error) {\n        errorMessage.value = 'Failed to load dashboard data';\n        console.error(error);\n      } finally {\n        loading.value = false;\n      }\n    };\n    const useMockData = () => {\n      dashboardData.emergency_alerts = [{\n        id: 1,\n        title: 'Typhoon Warning',\n        description: 'Severe typhoon expected.',\n        severity: 'high',\n        created_at: new Date()\n      }];\n    };\n    const formatDate = date => new Date(date).toLocaleString();\n    const viewAllUpdates = () => {\n      router.push({\n        name: 'ResidentDisasterUpdates'\n      });\n    };\n    const __returned__ = {\n      router,\n      loading,\n      errorMessage,\n      userDisplayName,\n      dashboardData,\n      fetchDashboard,\n      useMockData,\n      formatDate,\n      viewAllUpdates,\n      reactive,\n      ref,\n      onMounted,\n      get useRouter() {\n        return useRouter;\n      },\n      get residentDashboardService() {\n        return residentDashboardService;\n      },\n      get residentEmergencyAlertsService() {\n        return residentEmergencyAlertsService;\n      },\n      get residentPredictionsService() {\n        return residentPredictionsService;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["reactive","ref","onMounted","useRouter","residentDashboardService","residentEmergencyAlertsService","residentPredictionsService","router","loading","errorMessage","userDisplayName","dashboardData","total_evacuation_areas","families_evacuated","total_people","available_areas","full_areas","emergency_alerts","recent_disaster_updates","active_predictions","user","JSON","parse","localStorage","getItem","value","name","fetchDashboard","dashboardResponse","getDashboard","status","d","data","totalEvacuationAreas","totalFamilies","totalPeople","availableAreas","fu