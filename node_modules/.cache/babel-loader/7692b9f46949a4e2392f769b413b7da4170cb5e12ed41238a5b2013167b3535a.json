\"\r\n            class=\"flex items-center gap-2 px-2 lg:px-4 py-2 text-sm font-medium transition-colors duration-200 rounded-t-lg\"\r\n            :class=\"isActive(item.to) \r\n              ? 'bg-white/20 text-white border-b-2 border-green-300' \r\n              : 'text-indigo-100 hover:bg-white/10 hover:text-white'\"\r\n          >\r\n            <i :class=\"item.icon\" class=\"text-base\"></i>\r\n            <span class=\"hidden lg:inline\">{{ item.label }}</span>\r\n          </router-link>\r\n        </div>\r\n\r\n        <!-- Mobile Menu Button -->\r\n        <button \r\n          v-if=\"showMobileMenu\"\r\n          @click=\"mobileMenuOpen = !mobileMenuOpen\"\r\n          class=\"md:hidden p-2 text-indigo-200 hover:text-white transition-colors\"\r\n          :aria-label=\"mobileMenuOpen ? 'Close menu' : 'Open menu'\"\r\n          :key=\"'mobile-btn'\"\r\n        >\r\n          <i :class=\"mobileMenuOpen ? 'fa fa-times' : 'fa fa-bars'\" class=\"text-xl\"></i>\r\n        </button>\r\n\r\n        <!-- User Section - Right side -->\r\n        <div class=\"flex items-center gap-2 pl-2 border-l border-indigo-600 flex-shrink-0\">\r\n          <button \r\n            @click=\"logout\"\r\n            class=\"flex items-center gap-2 px-2 sm:px-3 py-1.5 rounded-lg bg-red-500/30 hover:bg-red-500/50 text-red-200 hover:text-white transition-colors duration-200 text-sm font-medium whitespace-nowrap\"\r\n          >\r\n            <i class=\"fa fa-sign-out\"></i>\r\n            <span class=\"hidden sm:inline\">Logout</span>\r\n          </button>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Mobile Menu - Dropdown -->\r\n      <transition\r\n        enter-active-class=\"transition-all duration-200\"\r\n        leave-active-class=\"transition-all duration-200\"\r\n        enter-from-class=\"opacity-0 -translate-y-2\"\r\n        leave-to-class=\"opacity-0 -translate-y-2\"\r\n      >\r\n        <div \r\n          v-show=\"mobileMenuOpen\"\r\n          class=\"md:hidden