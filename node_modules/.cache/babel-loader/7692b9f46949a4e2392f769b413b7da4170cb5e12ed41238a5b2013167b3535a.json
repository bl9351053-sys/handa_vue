rage.removeItem('token');\r\n  localStorage.removeItem('user_id');\r\n  localStorage.removeItem('user_email');\r\n  \r\n  // Clear axios header\r\n  import('../services/AuthService').then(({ default: AuthService }) => {\r\n    AuthService.setToken(null);\r\n  }).catch(() => {\r\n    // If import fails, continue anyway\r\n  });\r\n  \r\n  router.push({ name: 'Login' });\r\n}\r\n\r\n// Close menu when route changes\r\nwatch(() => route.path, () => {\r\n  mobileMenuOpen.value = false;\r\n}, { immediate: false });\r\n\r\nonMounted(() => {\r\n  // Reset menu on mount\r\n  mobileMenuOpen.value = false;\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n/* Ensure nav stays at top */\r\nnav {\r\n  width: 100%;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n}\r\n\r\n/* Smooth transitions */\r\na, button {\r\n  transition: all 0.2s ease;\r\n}\r\n\r\n/* Prevent menu from shifting layout */\r\n::-webkit-scrollbar {\r\n  width: 4px;\r\n}\r\n\r\n::-webkit-scrollbar-track {\r\n  background: transparent;\r\n}\r\n\r\n::-webkit-scrollbar-thumb {\r\n  background: rgba(255, 255, 255, 0.2);\r\n  border-radius: 2px;\r\n}\r\n\r\n::-webkit-scrollbar-thumb:hover {\r\n  background: rgba(255, 255, 255, 0.3);\r\n}\r\n</style>\r\n"],"mappings":";AAqFA,SAASA,QAAQ,EAAEC,SAAS,QAAQ,YAAY;AAChD,SAASC,GAAG,EAAEC,KAAK,EAAEC,SAAS,EAAEC,QAAQ,QAAQ,KAAK;;;;;;;IAErD,MAAMC,KAAK,GAAGN,QAAQ,CAAC,CAAC;IACxB,MAAMO,MAAM,GAAGN,SAAS,CAAC,CAAC;IAC1B,MAAMO,cAAc,GAAGN,GAAG,CAAC,KAAK,CAAC;IAEjC,MAAMO,cAAc,GAAGJ,QAAQ,CAAC,MAAM;MACpC;MACA,OAAO,IAAI;IACb,CAAC,CAAC;IAEF,MAAMK,KAAK,GAAG,CACZ;MAAEC,IAAI,EAAE,WAAW;MAAEC,KAAK,EAAE,WAAW;MAAEC,EAAE,EAAE,YAAY;MAAEC,IAAI,EAAE;IAAmB,CAAC,EACrF;MAAEH,IAAI,EAAE,aAAa;MAAEC,KAAK,EAAE,aAAa;MAAEC,EAAE,EAAE,cAAc;MAAEC,IAAI,EAAE;IAAa,CAAC,EACrF;MAAEH,IAAI,EAAE,kBAAkB;MAAEC,KAAK,EAAE,kBAAkB;MAAEC,EAAE,EAAE,mBAAmB;MAAEC,IAAI,EAAE;IAAY,CAAC,EACnG;MAAEH,IAAI,EAAE,YAAY;MAAEC,KAAK,EAAE,YAAY;MAAEC,EAAE,EAAE,aAAa;MAAEC,IAAI,EAAE;IAAiB,CAAC,EACtF;MAAEH,IAAI,EAAE,UAAU;MAAEC,KAAK,EAAE,UAAU;MAAEC,EAAE,EAAE,WA