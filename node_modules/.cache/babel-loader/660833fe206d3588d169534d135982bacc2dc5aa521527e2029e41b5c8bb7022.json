m transition-all duration-200\"\r\n            :class=\"isActive(item.to) \r\n              ? 'bg-white/25 text-white border-b-2 border-green-300' \r\n              : 'text-indigo-100 hover:bg-white/10 hover:text-white'\"\r\n          >\r\n            <i :class=\"[item.icon, 'text-base']\"></i>\r\n            <span class=\"hidden lg:inline\">{{ item.label }}</span>\r\n          </router-link>\r\n        </div>\r\n\r\n        <!-- Mobile Menu Button & User -->\r\n        <div class=\"flex items-center gap-2 sm:gap-4\">\r\n          <!-- Mobile Menu Toggle -->\r\n          <button \r\n            @click=\"toggleMenu\"\r\n            class=\"md:hidden p-2 text-indigo-200 hover:text-white transition\"\r\n            aria-label=\"Toggle menu\"\r\n          >\r\n            <i :class=\"mobileMenuOpen ? 'fa fa-times' : 'fa fa-bars'\" class=\"text-xl\"></i>\r\n          </button>\r\n\r\n          <!-- User Menu -->\r\n          <div class=\"flex items-center gap-2 sm:gap-3 pl-2 sm:pl-4 border-l border-indigo-600\">\r\n            <div class=\"hidden sm:block text-right text-sm\">\r\n              <p class=\"text-indigo-100 text-xs\">{{ userName }}</p>\r\n            </div>\r\n            <button \r\n              @click=\"logout\"\r\n              class=\"flex items-center gap-2 px-2 sm:px-3 py-1.5 rounded-lg bg-red-500/30 text-red-200 hover:bg-red-500/50 hover:text-white transition-all duration-200 text-sm font-medium whitespace-nowrap\"\r\n            >\r\n              <i class=\"fa fa-sign-out\"></i>\r\n              <span class=\"hidden sm:inline\">Logout</span>\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Mobile Menu - Fixed positioning -->\r\n      <div \r\n        v-show=\"mobileMenuOpen\"\r\n        class=\"md:hidden border-t border-indigo-600 py-2 space-y-1 bg-indigo-800/50 fixed left-0 right-0 top-16 w-full\"\r\n      >\r\n        <router-link \r\n          v-for=\"item in items\" \r\n          :key=\"item.name\